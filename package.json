{
  "name": "n8n-nodes-ai-media-gen",
  "version": "0.0.5",
  "description": "n8n node for AI media generation with extensible architecture. Supports ModelScope, Nano Banana (Gemini), and Doubao Seedream APIs.",
  "keywords": [
    "n8n",
    "n8n-node",
    "n8n-community-node-package",
    "ai",
    "image-generation",
    "image-editing",
    "modelscope",
    "qwen",
    "doubao",
    "seedream",
    "gemini"
  ],
  "license": "MIT",
  "homepage": "https://github.com/n8n-nodes-ai-media-gen",
  "author": {
    "name": "n8n-nodes-ai-media-gen contributors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/n8n-nodes-ai-media-gen.git"
  },
  "bugs": {
    "url": "https://github.com/n8n-nodes-ai-media-gen/issues"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "gulp build",
    "build:dev": "gulp build:dev",
    "dev": "gulp dev",
    "format": "prettier nodes --write",
    "lint": "eslint nodes package.json",
    "lint:fix": "eslint nodes package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -s",
    "test": "jest"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "nodes/credentials/modelScopeApi.credentials.js",
      "nodes/credentials/doubaoApi.credentials.js"
    ],
    "nodes": [
      "nodes/AIMediaGen.js"
    ]
  },
  "dependencies": {
    "n8n-workflow": "^1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "del": "^6.1.1",
    "eslint": "^8.50.0",
    "eslint-plugin-n8n-nodes-base": "^1.0.0",
    "gulp": "^4.0.2",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "jest": "^29.7.0",
    "n8n-core": "^1.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}